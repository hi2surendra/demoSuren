pipeline {
  agent any
  stages {

    stage('Checkout Source') {
      steps {
        echo 'Suren test Checkout Source'
      }
    }

    stage('Build') {
      steps {
        echo 'Suren test Build'
      }
    }


    stage('Build image') {
      steps{
        echo 'Suren test Build Image'
      }
    }

    stage('Push Image') {
      steps{
        echo 'Suren test Push Image'
      }
    }

    stage('Deploy App') {
      steps {
        echo 'Suren test Deploy App'
      }
    }
    
    stage('Release') {
      steps {
        echo 'Suren test Release App V1'
      }
    }
    
  }
}

